#!/usr/bin/env ruby

require_relative '../config/environment'



puts "Welcome to Tic-Tac-Toe"

game = StartGame.new
game.prep_game
game.launch_game
puts "would you like to play again?(\"Y\" or \"N\")"
input = gets.strip
if input == "Y"
  game = StartGame.new
  game.prep_game
  game.launch_game
end
